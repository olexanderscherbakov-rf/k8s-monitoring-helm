{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "annotations": {
            "type": "object",
            "properties": {
                "job": {
                    "type": "string"
                }
            }
        },
        "deployAsConfigMap": {
            "type": "boolean"
        },
        "extraLogProcessingStages": {
            "type": "string"
        },
        "fullnameOverride": {
            "type": "string"
        },
        "global": {
            "type": "object",
            "properties": {
                "platform": {
                    "type": "string"
                }
            }
        },
        "kubernetesApiGathering": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "excludeNamespaces": {
                    "type": "array"
                },
                "extraDiscoveryRules": {
                    "type": "string"
                },
                "fieldSelectors": {
                    "type": "array"
                },
                "labelSelectors": {
                    "type": "object"
                },
                "namespaces": {
                    "type": "array"
                },
                "nodeFieldSelectors": {
                    "type": "array"
                },
                "nodeLabelSelectors": {
                    "type": "object"
                },
                "nodes": {
                    "type": "array"
                }
            }
        },
        "labels": {
            "type": "object",
            "properties": {
                "app_kubernetes_io_name": {
                    "type": "string"
                }
            }
        },
        "labelsToKeep": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "lokiReceiver": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "openShiftClusterLogForwarder": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "namespaces": {
                            "type": "array"
                        }
                    }
                },
                "port": {
                    "type": "integer"
                }
            }
        },
        "nameOverride": {
            "type": "string"
        },
        "staticLabels": {
            "type": "object"
        },
        "staticLabelsFrom": {
            "type": "object"
        },
        "structuredMetadata": {
            "type": "object"
        },
        "volumeGathering": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "excludeNamespaces": {
                    "type": "array"
                },
                "extraDiscoveryRules": {
                    "type": "string"
                },
                "fieldSelectors": {
                    "type": "array"
                },
                "labelSelectors": {
                    "type": "object"
                },
                "namespaces": {
                    "type": "array"
                },
                "nodeFieldSelectors": {
                    "type": "array"
                },
                "nodeLabelSelectors": {
                    "type": "object"
                },
                "onlyGatherNewLogLines": {
                    "type": "boolean"
                },
                "podLogsPath": {
                    "type": "string"
                }
            }
        },
        "gatherMethod": {
            "enum": [
                "volumes",
                "kubernetesApi"
            ]
        }
    }
}
